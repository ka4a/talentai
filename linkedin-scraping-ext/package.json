{
  "name": "zookeep-ext",
  "version": "0.10.0",
  "description": "ZooKeep.io Extension",
  "scripts": {
    "build-zookeep": "NODE_ENV=production webpack --config src/zookeep/webpack.config.js",
    "start-zookeep": "NODE_ENV=development webpack --watch --config src/zookeep/webpack.config.js",
    "build-hccr": "NODE_ENV=production webpack --config src/hccr/webpack.config.js",
    "start-hccr": "NODE_ENV=development webpack --config src/hccr/webpack.config.js",
    "prettier": "prettier --write \"./src/**/*.{js,jsx,css,scss,json}\""
  },
  "devDependencies": {
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "prettier": "^2.3.2",
    "webpack": "4.28.4",
    "webpack-chrome-extension-reloader": "^1.2.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.2",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0"
  }
}
